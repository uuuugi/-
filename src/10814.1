#include <stdio.h>
#include <string.h>
#include <stdlib.h>

#define SIZE 100001

typedef struct {
	int age;
	char name[101];
	int cnt;
}user;


int compare(const void* a, const void* b)
{
	user* A = (user*)a;
	user* B = (user*)b;

	if (A->age == B->age)
		return A->cnt - B->cnt;
	else
		return A->age - B->age;
}

int main(void)
{
	int n;
	user list[SIZE];

	scanf("%d", &n);

	for (int i = 0; i < n; i++)
	{
		scanf("%d", &list[i].age);
		scanf("%s", &list[i].name);
		list[i].cnt = i + 1;
	}

	qsort(list, n, sizeof(user), compare);

	for (int i = 0; i < n; i++)
		printf("%d %s\n", list[i].age, list[i].name);

	return 0;
}
